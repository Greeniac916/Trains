versionPage = fs.open("Trains/version", "r")
version = tonumber(versionPage.readLine())
upVersionPage = http.get("https://raw.github.com/Greeniac916/Trains/master/version")
upVersion = tonumber(upVersionPage.readAll())
versionPage.close
if version < upVersion then
  print("Update Found... Updating")
  updateF = io.open("startup","w")
  file:write("shell.run('updater')")
  file:close
  os.reboot()
end
  
